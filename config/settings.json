{
    "global": {
        "general": {
            "download_path": "/mnt/c/Users/xipher/Documents/OrpheusDL/downloads",
            "download_quality": "hifi",
            "search_limit": 10
        },
        "artist_downloading": {
            "return_credited_albums": true,
            "separate_tracks_skip_downloaded": true
        },
        "formatting": {
            "album_format": "{name}{explicit}",
            "playlist_format": "{name}{explicit}",
            "track_filename_format": "{track_number}. {name}",
            "single_full_path_format": "{name}",
            "enable_zfill": true,
            "force_album_format": false
        },
        "codecs": {
            "proprietary_codecs": false,
            "spatial_codecs": true
        },
        "module_defaults": {
            "lyrics": "default",
            "covers": "default",
            "credits": "default"
        },
        "lyrics": {
            "embed_lyrics": true,
            "embed_synced_lyrics": false,
            "save_synced_lyrics": true
        },
        "covers": {
            "embed_cover": true,
            "main_compression": "high",
            "main_resolution": 1400,
            "save_external": false,
            "external_format": "png",
            "external_compression": "low",
            "external_resolution": 3000,
            "save_animated_cover": true
        },
        "playlist": {
            "save_m3u": true,
            "paths_m3u": "absolute",
            "extended_m3u": true
        },
        "advanced": {
            "advanced_login_system": false,
            "codec_conversions": {
                "alac": "flac",
                "wav": "flac"
            },
            "conversion_flags": {
                "flac": {
                    "compression_level": "5"
                }
            },
            "conversion_keep_original": false,
            "cover_variance_threshold": 8,
            "debug_mode": false,
            "disable_subscription_checks": false,
            "enable_undesirable_conversions": false,
            "ignore_existing_files": false,
            "ignore_different_artists": true,
            "allow_insecure_requests": false
        }
    },
    "extensions": {
        "logging": {
            "logger": {
                "path": "orpheus.log"
            }
        },
        "network_ai": {
            "assistant": {}
        }
    },
    "modules": {
        "soundcloud": {
            "web_access_token": ""
        },
        "beatsource": {
            "username": "",
            "password": ""
        },
        "qobuz": {
            "app_id": "$env:QOBUZ_APP_ID",
            "app_secret": "$env:QOBUZ_APP_SECRET",
            "quality_format": "{sample_rate}kHz {bit_depth}bit",
            "auth_token": "$env:QOBUZ_AUTH_TOKEN",
            "user_id": "$env:QOBUZ_USER_ID",
            "username": "$env:QOBUZ_USERNAME",
            "password": "$env:QOBUZ_PASSWORD"
        },
        "bugsmusic": {
            "username": "",
            "password": ""
        },
        "tidal": {
            "tv_atmos_token": "$env:TIDAL_TV_ATMOS_TOKEN",
            "tv_atmos_secret": "$env:TIDAL_TV_ATMOS_SECRET",
            "mobile_atmos_hires_token": "$env:TIDAL_MOBILE_ATMOS_HIRES_TOKEN",
            "mobile_hires_token": "$env:TIDAL_MOBILE_HIRES_TOKEN",
            "enable_mobile": true,
            "prefer_ac4": false,
            "fix_mqa": true
        },
        "beatport": {
            "username": "",
            "password": ""
        },
        "deezer": {
            "client_id": "$env:DEEZER_CLIENT_ID",
            "client_secret": "$env:DEEZER_CLIENT_SECRET",
            "bf_secret": "$env:DEEZER_BF_SECRET",
            "arl": "$env:DEEZER_ARL",
            "email": "$env:DEEZER_EMAIL",
            "password": "$env:DEEZER_PASSWORD"
        },
        "napster": {
            "api_key": "$env:NAPSTER_API_KEY",
            "customer_secret": "$env:NAPSTER_CUSTOMER_SECRET",
            "requested_netloc": "napster",
            "username": "$env:NAPSTER_USERNAME",
            "password": "$env:NAPSTER_PASSWORD"
        },
        "kkbox": {
            "kc1_key": "$env:KKBOX_KC1_KEY",
            "secret_key": "$env:KKBOX_SECRET_KEY",
            "email": "$env:KKBOX_EMAIL",
            "password": "$env:KKBOX_PASSWORD"
        },
        "nugs": {
            "username": "",
            "password": "",
            "client_id": "$env:NUGS_CLIENT_ID",
            "dev_key": "$env:NUGS_DEV_KEY"
        }
    }
}
